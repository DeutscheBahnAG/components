{
  "name": "@bahn-x/dbx-icons",
  "version": "0.7.0",
  "description": "DBX Icon components for React",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "directory": "components/icons",
    "url": "https://github.com/Bahn-X/dbx-web.git",
    "type": "git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
