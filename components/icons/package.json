    "core-js": "^3.6.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@bahn-x/dbx-foundation": ">=0.7.0",
    "@svgr/cli": "^5.4.0",
    "camelcase": "^6.0.0",
    "typescript": "^4.0.2"
  },
  "sideEffects": false
}
